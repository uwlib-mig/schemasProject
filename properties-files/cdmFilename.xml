<?xml version="1.0" encoding="UTF-8"?>
<property xmlns="https://uwlib-mig.github.io/schemasProject/xsd/"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
   xmlns:xs="http://www.w3.org/2001/XMLSchema"
   xsi:schemaLocation="https://uwlib-mig.github.io/schemasProject/xsd/
   https://uwlib-mig.github.io/schemasProject/xsd/properties_files.xsd">
   <uid>p12</uid>
   <labels>
      <platformIndependent>cdmFilename</platformIndependent>
      <dc>none</dc>
   </labels>
   <uwRequired>
      <!-- As with cdmFilePath, cdmNumber, Why do we say this property is UW required if we do not create these values? -->
      <uwStandalone>yes</uwStandalone>
      <uwObject>yes</uwObject>
      <uwItem>no</uwItem>
   </uwRequired>
   <cdm>
      <label>cdmfilename</label>
      <cdmDatatype>cdmAdministrative</cdmDatatype>
      <cdmLarge>no</cdmLarge>
      <searchable>no</searchable>
      <hidden>yes</hidden>
      <cdmRequired>yes</cdmRequired>
      <cdmControlledVocab>no</cdmControlledVocab>
   </cdm>
   <dspace>
      <dspaceProperty/>
   </dspace>
   <descriptions>
      <definition>
         <para>Unique filename for the resource; unique only within the context of the CONTENTdm digital collection the resource is a part of.</para>
      </definition>
      <instructions co="no">
         <para>Do not input. Automatically created by CONTENTdm.</para>
      </instructions>
      <instructions co="object">
         <para>Do not input. Automatically created by CONTENTdm.</para>
      </instructions>
      <instructions co="item">
         <para>Do not input. Automatically created by CONTENTdm.</para>
      </instructions>
   </descriptions>
   <examples>
      <example co="no">
         <para>0381826102001_553.jpg</para>
      </example>
      <example co="object">
         <para>0381826102001_553.jpg</para>
      </example>
      <example co="item">
         <para>0381826102001_553.jpg</para>
      </example>
   </examples>
   <additionalInfo>
      <generalNotes co="all">
         <para>CONTENTdm administrative field.</para>
         <para>Does NOT output in OAI-PMH.</para>
         <para>Not searchable in the cdm collection (default setting; can be changed); it does not appear to be an indexed field.</para>
      </generalNotes>
   </additionalInfo>
   <marc>
      <dcgMarcMapping/>
      <marcNotes>
         <para/>
      </marcNotes>
   </marc>
   <xsd>
      <typeInfo level="standalone object item">
         <typeAttribute>xs:string</typeAttribute>
      </typeInfo>
   </xsd>
   <rdf/>
   <history>
      <dateCreated>
         <single>1999-01-01</single>
      </dateCreated>
      <created4Coll>CONTENTdm</created4Coll>
      <justification>Needed a system-created unique identifier for each file/resource in a given digital collection.</justification>
      <changeLog>
         <change/>
         <change date="2017-10-23">changed //cdm/required to //cdm/cdmRequired and //cdm/uwRequired to assist in halting use of the cdm "required" feature while, at the same time, indicating that UW-cdm collections must use, or not use, a given property; also changed @xsi:schemaLocation.</change>
      </changeLog>
   </history>
</property>
