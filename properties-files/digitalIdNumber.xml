<?xml version="1.0" encoding="UTF-8"?>
<property xmlns="http://faculty.washington.edu/tgis/schemasProject/xsd4md"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema"
   xsi:schemaLocation="http://faculty.washington.edu/tgis/schemasProject/xsd4md http://faculty.washington.edu/tgis/schemasProject/xsd4md/schema-4-properties.xsd">
   <uid>p30</uid>
   <labels>
      <platformIndependent>digitalIdNumber</platformIndependent>
      <dc>identifier</dc>
   </labels>
   <uwRequired>
      <uwStandalone>yes</uwStandalone>
      <uwObject>yes</uwObject>
      <uwItem>no</uwItem>
   </uwRequired>
   <cdm>
      <label>Digital ID Number</label>
      <cdmDatatype>text</cdmDatatype>
      <cdmLarge>no</cdmLarge>
      <searchable>yes</searchable>
      <hidden>no</hidden>
      <cdmRequired>no</cdmRequired>
      <cdmControlledVocab>no</cdmControlledVocab>
   </cdm>
   <dspace>
      <dspaceProperty/>
   </dspace>
   <descriptions>
      <definition>
         <para>Unique number assigned to each digital object in CONTENTdm for identification purposes.</para>
      </definition>
      <instructions co="no">
         <para>Enter the order number.</para>
         <para>Order number sequence must be obtained from appropriate staff.</para>
      </instructions>
      <instructions co="object">
         <para>Enter the order number.</para>
         <para>Order number sequence must be obtained from appropriate staff.</para>
      </instructions>
      <instructions co="item">
         <para>Enter the order number.</para>
         <para>Order number sequence must be obtained from appropriate staff.</para>
      </instructions>
   </descriptions>
   <examples>
      <example co="no">
         <para>CVW051</para>
         <para>COS321</para>
         <para>ARC0443</para>
      </example>
      <example co="object">
         <para>OHC0847</para>
      </example>
      <example co="item">
         <para>OHC0847-Audio</para>
         <para>OHC084-Transcript</para>
         <para>OHC084-Video</para>
      </example>
      <customization co="object" dd="uwtoh">
         <para>OWTOH201701</para>
         <para>OWTOH201702</para>
      </customization>
      <customization co="item" dd="uwtoh">
         <para>OWTOH201802-Audio</para>
         <para>UWTOH201802-Image</para>
         <para>UWTOH201802-Transcript</para>
      </customization>
   </examples>
   <additionalInfo>
      <admin co="no">
         <para>Photograph collections also have this property but it is labeled "Order Number."</para>
      </admin>
   </additionalInfo>
   <marc>
      <dcgMarcMapping/>
      <marcNotes>
         <para/>
      </marcNotes>
   </marc>
   <xsd>
      <typeInfo level="standalone object">
         <simpleType>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
            </xs:restriction>
         </simpleType>
      </typeInfo>
      <typeInfo level="item">
         <typeAttribute>xs:string</typeAttribute>
      </typeInfo>
   </xsd>
   <rdf> </rdf>
   <history>
      <dateCreated>
         <range>
            <start>2012-01-01</start>
            <end>2018-03-23</end>
         </range>
      </dateCreated>
      <created4Coll>Special Collections Oral Histories Collections</created4Coll>
      <justification>Special Collections wants a identifier for each object in CONTENTdm</justification>
      <changeLog>
         <change/>
      </changeLog>
   </history>
</property>
