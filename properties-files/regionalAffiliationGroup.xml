<?xml version="1.0" encoding="UTF-8"?>
<property xmlns="https://uwlib-mig.github.io/schemasProject/xsd/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xsi:schemaLocation="https://uwlib-mig.github.io/schemasProject/xsd/
    https://uwlib-mig.github.io/schemasProject/xsd/properties_files.xsd">
    <uid>p111</uid>
    <labels>
        <platformIndependent>regionalAffiliationGroup</platformIndependent>
        <dc>coverage</dc>
    </labels>
    <uwRequired>
        <uwStandalone>no</uwStandalone>
        <uwObject>no</uwObject>
        <uwItem>no</uwItem>
    </uwRequired>
    <cdm>
        <label>Regional Affiliation (Group)</label>
        <cdmDatatype>text</cdmDatatype>
        <cdmLarge>no</cdmLarge>
        <searchable>yes</searchable>
        <hidden>no</hidden>
        <cdmRequired>no</cdmRequired>
        <cdmControlledVocab>no</cdmControlledVocab>
    </cdm>
    <dspace>
        <dspaceProperty></dspaceProperty>
    </dspace>
    <descriptions>
        <definition>
            <!-- this definition needs work?? -->
            <para>Location from where the group(s) represented in the resource derives.</para>
        </definition>
        <instructions co="no">
            <para>Enter the name of the place the group is from.</para>
            <para>Separate multiple entries using a semicolon.</para>
            <para>Use GeoNet (https://community.esri.com/) form of the name, if available. If not, use the most common form/spelling.</para>
        </instructions>
        <instructions co="object">
            <para>Enter the name of the place the group is from.</para>
            <para>Separate multiple entries using a semicolon.</para>
            <para>Use GeoNet (https://community.esri.com/) form of the name, if available. If not, use the most common form/spelling.</para>
        </instructions>
        <customization co="object" dd="greekdancing" resourceType="default">
            <para>Enter the name of the place the group is from.</para>
            <para>Separate multiple places using a semicolon.</para>
            <para>Record the form of each place name as found in the text &lt;a href="https://alliance-primo.hosted.exlibrisgroup.com/permalink/f/kjtuig/CP71232515330001451"&gt;A Nest of Gold&lt;/a&gt;.</para>
        </customization>
    </descriptions>
    <examples>
        <example co="no">
            <para>Skotoussa (Kentriki Makedonia, Greece)</para>
            <para>Iraklia (Kentriki Macedonia, Greece)</para>
        </example>
        <example co="object">
            <para>Skotoussa (Kentriki Makedonia, Greece)</para>
            <para>Iraklia (Kentriki Macedonia, Greece)</para>
        </example>
        <customization co="object" dd="greekdancing">
            <para>Kimisi (Greece)</para>
            <para>Skotoussa (Kentriki Makedonia, Greece)</para>
            <para>Iraklia (Kentriki Macedonia, Greece)</para>
            <para>Skotoussa (Kentriki Makedonia, Greece); Iraklia (Kentriki Macedonia, Greece)</para>
        </customization>
    </examples>
    <additionalInfo>
        <admin co="all">
            <para>In CONTENTdm, map to DC map = Coverage-Spatial.</para>
        </admin>
    </additionalInfo>
    <marc>
        <dcgMarcMapping></dcgMarcMapping>
    </marc>
    <xsd>
        <typeInfo level="standalone object">
            <typeAttribute>xs:string</typeAttribute>
        </typeInfo>
    </xsd>
    <rdf>
        <owlDatatypeProperty>yes</owlDatatypeProperty>
    </rdf>
    <history>
        <dateCreated><single>2020-06-03</single></dateCreated>
        <created4Coll>Greek Dances</created4Coll>
        <justification>Needed a field to describe the group associated with a dance depicted.</justification>
        <changeLog>
            <change date="2022-02-28">Per collection-manager request, change guidance to source place names from the text A Nest of Gold.</change>
            <change date="2022-05-11">Add example for Greek dances MAP</change>
        </changeLog>
    </history>
</property>













