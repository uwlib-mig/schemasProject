<?xml version="1.0" encoding="UTF-8"?>
<property xmlns="http://faculty.washington.edu/tgis/schemasProject/xsd4md"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xsi:schemaLocation="http://faculty.washington.edu/tgis/schemasProject/xsd4md http://faculty.washington.edu/tgis/schemasProject/xsd4md/schema-4-properties.xsd">
    <uid>p102</uid>
    <labels>
        <platformIndependent>filmmaker</platformIndependent>
        <dc>creator</dc>
    </labels>
    <uwRequired>
        <uwStandalone>no</uwStandalone>
        <uwObject>no</uwObject>
        <uwItem>no</uwItem>
    </uwRequired>
    <cdm>
        <label>Filmmaker</label>
        <cdmDatatype>text</cdmDatatype>
        <cdmLarge>no</cdmLarge>
        <searchable>yes</searchable>
        <hidden>no</hidden>
        <cdmRequired>no</cdmRequired>
        <cdmControlledVocab>no</cdmControlledVocab>
    </cdm>
    <dspace>
        <dspaceProperty></dspaceProperty>
    </dspace>
    <descriptions>
        <definition>
            <para>Entity responsible for filming the video.</para>
        </definition>
        <instructions co="no">
            <para>Enter name of person who shot the video.</para>
            <para>Enter the name in inverted form.</para>
        </instructions>
        <instructions co="object">
            <para>Enter name of person who shot the video.</para>
            <para>Enter the name in inverted form.</para>
            <para>Use the form of the name in the LC Name Authority File when possible.</para>
        </instructions>
    </descriptions>
    <examples>
        <example co="no"><para>Hunt, Yvonne (Yvonne M.)</para><para>Karakostas, Christos</para></example>
        <example co="object"><para>Hunt, Yvonne (Yvonne M.)</para><para>Karakostas, Christos</para></example>
    </examples>
    <additionalInfo></additionalInfo>
    <marc>
        <dcgMarcMapping></dcgMarcMapping>
    </marc>
    <xsd>
        <typeInfo level="standalone object">
            <typeAttribute>xs:string</typeAttribute>
        </typeInfo>
    </xsd>
    <rdf>
        <owlDatatypeProperty>yes</owlDatatypeProperty>
    </rdf>
    <history>
        <dateCreated>
            <single>2020-05-18</single>
        </dateCreated>
        <created4Coll>Greek Dances</created4Coll>
        <justification>Needed a field to record the name of the person who recorded the event.</justification>
        <changeLog>
            <change></change>
        </changeLog>
    </history>
