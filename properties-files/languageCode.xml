   </uwRequired><cdm>
      <label>Language Code</label>
      <cdmDatatype>text</cdmDatatype>
      <cdmLarge>no</cdmLarge>
      <searchable>yes</searchable>
      <hidden>yes</hidden>
      <cdmRequired>no</cdmRequired>
      <cdmControlledVocab>no</cdmControlledVocab>
   </cdm><dspace>
      <dspaceProperty/>
   </dspace><descriptions>
      <definition>
         <para>A language of the resource expressed as a code.</para>
      </definition>
      <instructions co="no">
         <para>Enter a code for a language used in the resource.</para>
         <para>Enter a code from ISO 639-3.</para>
      </instructions>
      <instructions co="object">
         <para>Enter a code for a language used in the resource.</para>
         <para>Enter a code from ISO 639-3.</para>
      </instructions>
      <instructions co="item">
         <para>Enter a code for a language used in the resource.</para>
         <para>Enter a code from ISO 639-3.</para>
      </instructions>
      <customization co="object" dd="uwtoh">
         <para>Enter a code for a language(s) used in the interview using a code from ISO 639-3.</para>
      </customization>
   </descriptions>
   <examples>
      <example co="object">
         <para>eng</para>
         <para>spa</para>
      </example>
      <example co="item">
         <para>eng</para>
         <para>spa</para>
      </example>
      <example co="no">
         <para>eng</para>
         <para>spa</para>
      </example>
   </examples>
   <additionalInfo>
      <admin co="object">
         <para>Option for dSpace (not currently in input instructions): instead of using ISO 639-3, use the default DSpace rendering of languages: a 2 letter code from ISO 639-1 followed by an underscore and then an optional 2 letter country code from ISO 3166-1.</para>
      </admin>
   </additionalInfo><marc>
