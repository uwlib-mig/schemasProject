    <uid>p96</uid>
    <labels>
        <platformIndependent>Producer</platformIndependent>
        <dc>contributor</dc>
    </labels>
    <uwRequired>
        <uwStandalone>no</uwStandalone>
        <uwObject>no</uwObject>
        <uwItem>no</uwItem>
