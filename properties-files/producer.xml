    <cdm>
        <label>Producer</label>
        <cdmDatatype>text</cdmDatatype>
        <cdmLarge>no</cdmLarge>
        <searchable>yes</searchable>
        <hidden>no</hidden>
        <cdmRequired>no</cdmRequired>
        <cdmControlledVocab>no</cdmControlledVocab>
    </cdm>
    <dspace>
        <dspaceProperty></dspaceProperty>
    </dspace>
    <descriptions>
        <definition><para>A person, family, or organization responsible for most of the business aspects of a production.</para></definition>
        <instructions co="no">
            <para>Input name of the producer.</para>
            <para>Use LC Authority File for form of name, if available.</para>
            <para>Input Lastname, Firstname for all personal names.</para>
        </instructions>
        <instructions co="object">
            <para>Input name of the producer.</para>
            <para>Use LC Authority File for form of name, if available.</para>
            <para>Input Lastname, Firstname for all personal names.</para>
        </instructions>
        <instructions co="item">
            <para>Input name of the producer if that producer that should be indexed at the item level.</para>
            <para>Use LC Authority File for form of name, if available.</para>
            <para>Input Lastname, Firstname for all personal names.</para>
        </instructions>
    </descriptions>
    <examples>
        <example co="no">
            <para>Lesbian Feminist Radio Collective</para>
        </example>
        <example co="object">
            <para>Lesbian Feminist Radio Collective</para>
        </example>
        <example co="item">
            <para>Lesbian Feminist Radio Collective</para>
        </example>
    </examples>
